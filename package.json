{
  "name": "app-dependencies",
  "version": "1.0.0",
  "private": true,
  "main": "back-end/index.js",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "ci:back": "cd back-end && npm run start:test",
    "ci:front": "cd front-end && npm start",
    "start:ci": "npm-run-all --parallel ci:*",
    "build:ui": "cd front-end && rm -rf build && npm run build --prod && cp -r build ../back-end/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
